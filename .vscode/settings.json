{
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[prisma]": {
    "editor.formatOnSave": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#664d46",
    "activityBar.activeBorder": "#1a261d",
    "activityBar.background": "#664d46",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#1a261d",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#483631",
    "statusBar.border": "#483631",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#664d46",
    "titleBar.activeBackground": "#483631",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#483631",
    "titleBar.inactiveBackground": "#48363199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#483631"
}
