{
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[prisma]": {
    "editor.formatOnSave": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2385f2",
    "activityBar.activeBorder": "#96094c",
    "activityBar.background": "#2385f2",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#96094c",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#0d6cd5",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2385f2",
    "titleBar.activeBackground": "#0d6cd5",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0d6cd599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#0d6cd5"
}
